<extension xmlns="http://ns.adobe.com/air/extension/3.1">     <id>net.digitalprimates.volume</id>     <versionNumber>0.0.1</versionNumber>     <platforms>         <platform name="Android-ARM">            <applicationDeployment>                <nativeLibrary>libAndroidVolumeLib.jar</nativeLibrary>                <initializer>net.digitalprimates.volume.VolumeExtension</initializer>            </applicationDeployment>         </platform>		<platform name="iPhone-ARM">             <applicationDeployment>                 <nativeLibrary>libIOSVolumeLib.a</nativeLibrary>                 <initializer>VolumeExtensionInitializer</initializer>            </applicationDeployment>        </platform>        <platform name="default">            <applicationDeployment/>        </platform>    </platforms> </extension>